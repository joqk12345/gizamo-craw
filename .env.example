# Multi-tenant boot list (single process, multiple agents)
AGENT_TENANTS=news,strategic

# Tenant: news
NEWS_AGENT_ROLE=news
NEWS_TELEGRAM_BOT_TOKEN=your_news_bot_token
NEWS_TELEGRAM_ALLOWED_USER_IDS=123456789
# NEWS_PERSONA_WORKSPACE=.
# NEWS_SOUL_FILE=SOUL.news.md
# NEWS_IDENTITY_FILE=IDENTITY.news.md
# NEWS_USER_FILE=USER.news.md
# NEWS_TELEGRAM_ALLOWED_CHAT_IDS=123456789
# NEWS_REPORT_BASE_PATH=reports/news

# Tenant: strategic
STRATEGIC_AGENT_ROLE=strategic
STRATEGIC_TELEGRAM_BOT_TOKEN=your_strategic_bot_token
STRATEGIC_TELEGRAM_ALLOWED_USER_IDS=123456789
# STRATEGIC_STRATEGIC_INSUFFICIENT_SIGNAL_THRESHOLD=0.35
# STRATEGIC_PERSONA_WORKSPACE=.
# STRATEGIC_SOUL_FILE=SOUL.strategic.md
# STRATEGIC_IDENTITY_FILE=IDENTITY.strategic.md
# STRATEGIC_USER_FILE=USER.strategic.md
# STRATEGIC_TELEGRAM_ALLOWED_CHAT_IDS=123456789
# STRATEGIC_REPORT_BASE_PATH=reports/strategic

# Legacy fallback (for single-tenant compatibility only)
# AGENT_ROLE=news
# TELEGRAM_BOT_TOKEN=your_bot_token
# TELEGRAM_ALLOWED_USER_IDS=123456789
# TELEGRAM_ALLOWED_CHAT_IDS=123456789
TELEGRAM_LONG_POLL_TIMEOUT_SEC=10
# Set 1 if your network keeps resetting long polling connections:
TELEGRAM_FORCE_SHORT_POLL=0
# Transport: fetch | curl (curl is more robust in some proxy/network environments)
TELEGRAM_TRANSPORT=fetch
# Optional (if missing, bot still starts but summarize/analyze tasks return config hint):
# OPENROUTER_API_KEY=your_openrouter_api_key
# Alias also supported: openrouter_API_KEY / openrouter_api_key
OPENROUTER_MODEL=stepfun/step-3.5-flash:free
# Optional: if primary model is blocked by region, auto fallback in order (comma-separated)
OPENROUTER_FALLBACK_MODELS=deepseek/deepseek-chat:free,meta-llama/llama-3.3-70b-instruct:free,mistralai/mistral-7b-instruct:free
# Optional (if missing, bot returns only TG short reply without report link):
# GITHUB_TOKEN=your_github_token
# GITHUB_REPO=owner/repo
# Alias also supported for repo: GITHUB_REPOSITORY=owner/repo
GITHUB_BRANCH=main
POLL_INTERVAL_MS=1500
REPORT_BASE_PATH=reports

# Optional (if your network needs proxy):
# NODE_USE_ENV_PROXY=1
# HTTPS_PROXY=http://127.0.0.1:7890

# Heartbeat proactive巡检（默认开启）
HEARTBEAT_ENABLED=1
# 默认 30 分钟
HEARTBEAT_INTERVAL_MS=1800000
# 可选：健康检查 URL（逗号分隔）；也可直接写在 HEARTBEAT.md 的条目里
# HEARTBEAT_HEALTH_URLS=https://example.com/health,https://api.example.com/ping
